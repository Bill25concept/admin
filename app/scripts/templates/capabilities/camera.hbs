<h4>Camera</h4>

<table class="table">
    <tr>
        <th>Timestamp</th>
        <th>From</th>
        <th>Command</th>
    </tr>

    {{#each command in view.commands}}
    <tr>
        <td>{{command.timestampString}}</td>
        <td>{{command.from}}</td>
        <td>{{command.body.command}}</td>
    </tr>
    {{/each}}

</table>

<div class="row">
    <div class="btn-group" data-toggle="buttons-radio">
        <button type="button" class="btn btn-primary">Snapshot</button>
        <button type="button" class="btn btn-primary">Motion</button>
    </div>
</div>
